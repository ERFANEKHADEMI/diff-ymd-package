{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true,
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "prettier", // Add Prettier as an extension
  ],
  "plugins": ["jest", "prettier"], // Include Prettier as a plugin
  "parserOptions": {
    "ecmaVersion": 12,
  },
  "rules": {
    "prettier/prettier": "error", // Enforce Prettier rules
    // Add other project-specific rules here
  },
}
